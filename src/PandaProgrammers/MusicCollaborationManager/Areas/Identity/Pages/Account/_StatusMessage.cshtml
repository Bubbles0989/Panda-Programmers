@model string

@if (!String.IsNullOrEmpty(Model))
{
    var statusMessageClass = Model.StartsWith("Error") ? "danger" : "success";
    <h1 class="text-center text-3xl pb-3">@Model</h1>
    <button type="button" class="w-full flex-shrink-0 bg-gray-800 hover:bg-gray-600 text-white font-bold py-2 px-4 border-b-4 border-gray-900 hover:border-gray-700 rounded">
        <a href="~/">Click here to go to landing page</a>
    </button>
}
